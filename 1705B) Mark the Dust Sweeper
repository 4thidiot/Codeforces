#include <bits/stdc++.h>
using namespace std;

int main() 
{
	int t;
	cin>>t;
	
	int n;
	cin>>n;
	int a[n][t];
    int count=0;
    
    for(int i=0; i<t; i++)
    {
        for(int j=0; j<n; j++)
        {
            cin>>a[j][i];
        }
    }
    
    for(int i=0; i<t; i++)
    {
        for(int j=0; j<n-1; j++)
        {
            //  count+=a[j][i];
            //  if(a[j][i]==0)
            //     count+=1;
            
            //  if(a[0][t]!=0)
            //     count+=a[0][0];    
            
            count+=a[j][i]; 
            if(a[j][i]==0 && j!=0)  
                count=count+1;
               
           
        }
        cout<<count<<endl;
        count=0;
    }
    
    
    
        
       
    

}


//

#include <bits/stdc++.h>
using namespace std;

int main() 
{
	int t;
	cin>>t;
	
	int n;
	int count=0;
	int l;
	
	for(int i=0; i<t; i++)
	{
	    cin>>n;
	    for(int j=0; j<n; j++)
	    {
	        if(j!=n)
	        {    cin>>l;
	             count+=l;
	        }     
	        
	        if(l==0 && j!=0)
	            count+=1;
	    }
	    cout<<count<<endl;
	}

}
