#include <bits/stdc++.h>
using namespace std;

int main() {
	
int N;
cin>>N;
int Arr[N];
int even=0, odd=0, positive=0, negative=0;

for(int i=0; i<N; i++)
{
    cin>>Arr[i];
}


for(int i=0; i<N; i++)
{
    if(Arr[i]%2==0)
    even++;
    else
    odd++;
    if(Arr[i]>0)
    positive++;
    else if(Arr[i]<0)
    negative++;
}
      
     cout<<"Even: "<<even<<endl;
     cout<<"Odd: "<<odd<<endl;
     cout<<"Positive: "<<positive<<endl;
     cout<<"Negative: "<<negative<<endl;
}

// https://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/C
