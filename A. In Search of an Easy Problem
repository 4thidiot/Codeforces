#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int n;
	cin>>n;
	int Arr[n];
	int c=0;
	
	for(int i=0; i<n; i++)
	cin>>Arr[i];
	
	for(int i=0; i<n; i++)
	{
	    if(Arr[i]==1)
	    {
	        c++;
	        break;
	    }
	}
    if(c==1)
        cout<<"HARD";
    else
        cout<<"EASY";
}

// https://codeforces.com/problemset/problem/1030/A
