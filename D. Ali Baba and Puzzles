#include <bits/stdc++.h>
using namespace std;

int main() {
    
long long int a, b, c;      //using long long int for all variables to avoid overflow
long long int d;
cin>>a>>b>>c>>d;

if(a+b-c==d)            //3C2*2=6 ways
  cout<<"YES";
else if(a+b*c==d)
  cout<<"YES";
else if(a-b+c==d)
  cout<<"YES";  
else if(a-b*c==d)
  cout<<"YES"; 
else if(a*b+c==d)
  cout<<"YES"; 
else if(a*b-c==d)
  cout<<"YES";   
 
else
  cout<<"NO";
  
return 0;

}

/*
Cases
a+b-c
a+b*c
a-b+c
a-b*c
a*b+c
a*b-c
*/
