//working code:

#include <bits/stdc++.h>
using namespace std;

int main() 
{
	
 int a, b, c, max, min;
 cin>>a>>b>>c;
 max=a;
 min=a;
 
 
 if(min>b)
 min = b;
 if(min>c)
 min = c;
 
 if(max<b)
 max = b;
 if(max<c)
 max = c;



 cout<<min<<" "<<max;

    return 0;
}

// https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/K
